{
    "name": "casinsight",
    "version": "0.0.1",
    "description": "A project starter or template to bootstrap nodejs projects and packages with Typescript, eslint and more.",
    "repository": {
        "type": "git",
        "url": "https://github.com/hevar/node-ts-starter.git"
    },
    "author": {
        "name": "Hevar",
        "url": "https://h3.nu"
    },
    "keywords": [
        "typescript-template",
        "npm-boilerplate",
        "typescript-boilderplate",
        "typescript-starter",
        "node-starter",
        "node-boilerplate",
        "tsup-boilerplate",
        "tsup-starter",
        "typescript",
        "javascript",
        "tsup",
        "eslit"
    ],
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts"
        },
        "./package.json": "./package.json"
    },
    "typesVersions": {
        "*": {}
    },
    "files": [
        "dist/**/*",
        "README.md"
    ],
    "scripts": {
        "build": "tsup",
        "build:watch": "tsup --watch",
        "dev": "tsx watch ./src",
        "prettier": "prettier --write .",
        "eslint": "eslint .",
        "eslint:fix": "eslint . --fix",
        "test": "vitest run",
        "test:coverage": "vitest run --coverage",
        "test:watch": "vitest watch"
    },
    "devDependencies": {
        "@types/node": "^20.6.3",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^7.0.1",
        "eslint": "^8.56.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-config-standard-with-typescript": "^43.0.1",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-n": "^16.6.2",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-sonarjs": "^0.24.0",
        "prettier": "^3.0.3",
        "tsup": "^7.2.0",
        "tsx": "^4.7.1",
        "typescript": "^5.2.2",
        "vitest": "^0.33.0"
    },
    "publishConfig": {
        "access": "public"
    },
    "release": {
        "branches": [
            "main"
        ]
    },
    "dependencies": {
        "@vitest/coverage-v8": "^0.34.4"
    }
}
